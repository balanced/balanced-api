{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "pattern": "(CC|BA)[a-zA-Z0-9]{16,32}"
        },
        "href": {
            "type": "string",
            "format": "uri"
        },
        "links": {
            "type": "object",
            "properties": {},
            "additionalProperties": false
        }
    },
    "required": [
        "id",
        "href",
        "links"
    ],
    "additionalProperties": false
}