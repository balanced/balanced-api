{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "pattern": "NT[a-zA-Z0-9]{16,32}"
        },
        "href": {
            "type": "string",
            "format": "uri"
        },
        "created_at": {
            "description": "`ISO 8601 <http://www.w3.org/QA/Tips/iso-date>`_ date of when this bank account was tokenized.",
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        },
        "network": {
            "type": "string"
        },
        "meta": {
            "$ref": "meta.json"
        },
        "links": {
            "type": "object",
            "properties": {
                "customer": {
                    "description": "Customer this bank_account is associated with",
                    "type": [
                        "null",
                        "string"
                    ],
                    "pattern": "(CU|AC)[a-zA-Z0-9]{16,32}"
                }
            },
            "required": [
                "customer"
            ],
            "additionalProperties": false
        }
    },
    "additionalProperties": false,
    "required": [
        "id",
        "href",
        "created_at",
        "updated_at",
        "network",
        "meta",
        "links"
    ]
}