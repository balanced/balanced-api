{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "pattern": "(CU|AC)[a-zA-Z0-9]{16,32}"
        },
        "href": {
            "type": "string",
            "format": "uri"
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time"
        },
        "name": {
            "type": [
                "null",
                "string"
            ]
        },
        "business_name": {
            "type": [
                "null",
                "string"
            ]
        },
        "email": {
            "type": [
                "null",
                "string"
            ],
            "format": "email"

        },
        "phone": {
            "type": [
                "null",
                "string"
            ],
            "format": "phone"
        },
        "ssn_last4": {
            "type": [
                "null",
                "string"
            ],
            "pattern": "x{4}"
        },
        "ein": {
            "type": [
                "null",
                "string"
            ],
            "pattern": "[0-9]{9}"
        },
        "dob_month": {
            "type": [
                "null",
                "integer"
            ],
            "minimum": 1,
            "maximum": 12
        },
        "dob_year": {
            "type": [
                "null",
                "integer"
            ]
        },
        "merchant_status": {
            "type": "string",
            "enum": [
                "need-more-information",
                "underwritten",
                "rejected",
		"no-match"
            ]
        },
        "address": {
            "$ref": "address.json"
        },
        "meta": {
            "type": "object"
        },
        "links": {
            "type": "object",
            "properties": {
                "source": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "pattern": "(CC|BA)[a-zA-Z0-9]{16,32}"
                },
                "destination": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "pattern": "BA[a-zA-Z0-9]{16,32}"
                }
            },
            "required": [
                "source",
                "destination"
            ],
            "additionalProperties": false
        }
    },
    "required": [
        "id",
        "href",
        "created_at",
        "updated_at",
        "name",
        "business_name",
        "email",
        "phone",
        "ssn_last4",
        "ein",
        "dob_month",
        "dob_year",
        "merchant_status",
        "address",
        "meta",
        "links"
    ],
    "additionalProperties": false
}
