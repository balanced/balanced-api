{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "links": {
            "type": "object",
            "properties": {
                "disputes.transaction": {
                    "type": "string",
                    "format": "uri",
                    "pattern": "/resources/{disputes.transaction}"
                },
                "disputes.events": {
                    "type": "string",
                    "format": "uri",
                    "pattern": "/disputes/{disputes.id}/events"
                },
                "disputes.dispute_documents": {
                    "type": "string",
                    "format": "uri",
                    "pattern": "/disputes/{disputes.id}/dispute_documents"
                }
            },
            "required": [
                "disputes.transaction",
                "disputes.events",
                "disputes.dispute_documents"
            ]
        },
        "meta": {
            "type": "object"
        },
        "disputes": {
            "items": {
                "$ref": "_models/dispute.json"
            },
            "type": "array",
            "minItems": 0,
            "uniqueItems": true
        }
    },
    "required": [
        "disputes"
    ]
}