{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "required": true,
            "pattern": "CU[a-zA-Z0-9]{16,32}"
        },
        "href": {
            "type": "string",
            "format": "uri",
            "required": true
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "required": true
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "required": true
        },
        "name": {
            "type": [
                "null",
                "string"
            ],
            "required": true
        },
        "business_name": {
            "type": [
                "null",
                "string"
            ],
            "required": true
        },
        "email": {
            "type": [
                "null",
                "string"
            ],
            "format": "email",
            "required": true
        },
        "phone": {
            "type": [
                "null",
                "string"
            ],
            "format": "phone",
            "required": true
        },
        "ssn_last4": {
            "type": [
                "null",
                "string"
            ],
            "pattern": "x{4}",
            "required": true
        },
        "ein": {
            "type": [
                "null",
                "string"
            ],
            "pattern": "[0-9]{9}",
            "required": true
        },
        "dob_month": {
            "type": [
                "null",
                "integer"
            ],
            "minimum": 1,
            "maximum": 12,
            "required": true
        },
        "dob_year": {
            "type": [
                "null",
                "integer"
            ],
            "required": true
        },
        "merchant_status": {
            "type": "string",
            "enum": [
                "need-more-information",
                "underwritten",
                "rejected"
            ],
            "required": true
        },
        "address": {
            "$ref": "address.json",
            "required": true
        },
        "meta": {
            "type": "object",
            "required": true
        },
        "links": {
            "type": "object",
            "properties": {
                "source": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "format": "uri",
                    "required": true
                },
                "destination": {
                    "type": [
                        "null",
                        "string"
                    ],
                    "format": "uri",
                    "required": true
                }
            },
            "required": true,
            "additionalProperties": false
        }
    },
    "additionalProperties": false
}