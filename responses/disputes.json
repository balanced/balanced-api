{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "links": {
            "type": "object",
            "properties": {
                "disputes.transaction": {
                    "type": "string",
                    "format": "uri",
                    "pattern": "/disputes/{disputes.id}/transaction",
                    "required": true
                },
                "disputes.evidence": {
                    "type": "string",
                    "format": "uri",
                    "pattern": "/disputes/{disputes.id}/evidence",
                    "required": true
                }
            },
            "required": false
        },
        "meta": {
            "type": "object",
            "required": false
        },
        "disputes": {
            "items": {
                "$ref": "_models/dispute.json"
            },
            "type": "array",
            "minItems": 0,
            "uniqueItems": true,
            "required": true
        }
    }
}
