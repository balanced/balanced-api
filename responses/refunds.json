{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "links": {
            "type": "object",
	    "properties": {
		"refunds.debit": {
		    "type": "string",
		    "format": "uri",
		    "required": true
		},
		"refunds.order": {
		    "type": "string",
		    "format": "uri",
		    "required": true
		}
	    }
            "required": false
        },
        "meta": {
            "type": "object",
            "required": false
        },
        "refunds": {
            "items": {
                "$ref": "_models/refunds.json"
            },
            "type": "array",
            "minItems": 1,
            "uniqueItems": true
        }
    }
}
